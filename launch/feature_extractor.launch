<launch>
  <arg name="manager" default="feature_extractor_manager" />
  <arg name="standalone" default="false" />
  <arg name="image" />

  <node if="$(arg standalone)" name="$(arg manager)" pkg="nodelet" type="nodelet" args="manager" output="screen" />

  <node name="feature_extractor" pkg="nodelet" type="nodelet" args="load feature_extraction/FeatureExtractor $(arg manager)" output="screen" >
    <remap from="image" to="$(arg image)" />
    <param name="max_num_key_points" value="500" />
    <param name="feature_extractor" value="SURF" />
    <param name="show_image" value="true" />
  </node>

</launch>
